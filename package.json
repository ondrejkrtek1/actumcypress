{
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "cypress": "^13.7.2",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.1"
  },
    "cypress-cucumber-preprocessor": {
      "messages": {
        "enabled": true,
        "output": "./reports/cucumber-messages.ndjson"
      },
      "html": {
        "enabled": true,
        "output": "./reports/html/index.html"
      },
      "stepDefinitions": [
        "cypress/step-definitions/**/*.steps.{js,ts}"
      ],
      "filterSpecs": true,
      "omitFiltered": true,
      "nonGlobalStepDefinitions": false
    }
    
  
}
